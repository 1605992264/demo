		   </if>
           <if test="where.delFlag != null and where.delFlag != ''"> 
		  		AND del_flag = #{where.delFlag}
		   </if>
		</where>
	</update>
	
	<update id="removeByWhere">
		UPDATE flow_task_actor 
		SET `del_flag` = '1'
		<where> 
