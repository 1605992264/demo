<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.txdata.flow.dao.TaskActorDao">

    <sql id="taskActorColumns">
    	a.`id` AS "id",
    	a.`task_id` AS "taskId",
    	a.`varable` AS "varable",
    	a.`url` AS "url",
    	a.`create_date` AS "createDate",
    	a.`create_by` AS "createBy",
    	a.`del_flag` AS "delFlag",
    	a.`actor_id` AS "actorId",
    	a.flag AS flag
    </sql>
    
    <sql id="leftJion">
    </sql>
    
	<select id="get" resultType="TaskActorDO">
		SELECT  
		<include refid="taskActorColumns"/>
		FROM flow_task_actor a
		<include refid="leftJion"/>
		WHERE a.id = #{value}
	</select>

	<select id="list" resultType="TaskActorDO">
		SELECT 
        <include refid="taskActorColumns"/>
        FROM flow_task_actor a
        <include refid="leftJion"/>
        <where> 
           <if test="entity.id != null and entity.id != ''"> 
		  		AND a.id = #{entity.id}
		   </if>		
           <if test="entity.taskId != null and entity.taskId != ''"> 
		  		AND a.task_id = #{entity.taskId}
		   </if>		
           <if test="entity.varable != null and entity.varable != ''"> 
		  		AND a.varable = #{entity.varable}
		   </if>		
           <if test="entity.url != null and entity.url != ''"> 
		  		AND a.url = #{entity.url}
		   </if>		
           <if test="entity.createDate != null and entity.createDate != ''"> 
		  		AND a.create_date = #{entity.createDate}
		   </if>		
           <if test="entity.createBy != null and entity.createBy != ''"> 
		  		AND a.create_by = #{entity.createBy}
		   </if>
           <if test="entity.delFlag != null and entity.delFlag != ''"> 
		  		AND a.del_flag = #{entity.delFlag}
		   </if>
		   <if test="entity.delFlag == null or entity.delFlag == ''"> 
		  		AND a.del_flag = '0'
		   </if>
            <if test="entity.actorId != null and entity.actorId != ''">
                AND a.actor_id = #{entity.actorId}
            </if>
		</where>
	</select>
	
	<insert id="insert">
		INSERT INTO flow_task_actor
		<trim prefix="(" suffix=")" suffixOverrides="," >
			<if test="id != null">`id`,</if>
			<if test="taskId != null">`task_id`,</if>
			<if test="varable != null">`varable`,</if>
			<if test="url != null">`url`,</if>
            <if test="createDate != null">`create_date`,</if>
			<if test="createBy != null">`create_by`,</if>
			<if test="delFlag != null">`del_flag`,</if>
            <if test="actorId != null">`actor_id`,</if>
            <if test="flag != null">`flag`,</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides="," >
			<if test="id != null">#{id},</if>
			<if test="taskId != null">#{taskId},</if>
			<if test="varable != null">#{varable},</if>
			<if test="url != null">#{url},</if>
			<if test="createDate != null">#{createDate},</if>
			<if test="createBy != null">#{createBy},</if>
			<if test="delFlag != null">#{delFlag},</if>
            <if test="actorId != null">#{actorId},</if>
            <if test="flag != null">#{flag},</if>
		</trim>
	</insert>
	 
	<update id="update">
		UPDATE flow_task_actor 
		<set>
				<if test="taskId != null">`task_id` = #{taskId},</if>
				<if test="varable != null">`varable` = #{varable},</if>
				<if test="url != null">`url` = #{url},</if>
				<if test="createDate != null">`create_date` = #{createDate},</if>
				<if test="createBy != null">`create_by` = #{createBy},</if>
				<if test="delFlag != null">`del_flag` = #{delFlag}</if>
		</set>
		WHERE id = #{id} 
	</update>
	
	<update id="remove">
		UPDATE flow_task_actor 
		SET `del_flag` = '1'
		WHERE id = #{value}
	</update>
	
	<update id="batchRemove">
		UPDATE flow_task_actor 
		SET `del_flag` = '1'
		WHERE id IN 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</update>
	
	<delete id="delete">
		DELETE FROM flow_task_actor 
		WHERE id = #{value}
	</delete>
	
	<delete id="batchDelete">
		DELETE FROM flow_task_actor 
		WHERE id IN 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>
	
	<insert id="batchInsert">
		INSERT INTO flow_task_actor
		(
			`id`,
			`task_id`,
			`varable`,
			`url`,
			`create_date`,
			`create_by`,
			`del_flag`
		)
		VALUES
		<foreach item="taskActor" collection="list" separator=",">
		(
			#{taskActor.id},
			#{taskActor.taskId},
			#{taskActor.varable},
			#{taskActor.url},
			#{taskActor.createDate},
			#{taskActor.createBy},
			#{taskActor.delFlag}
		)
		</foreach>
	</insert>
	 
	<update id="batchUpdate">
		<foreach item="taskActor" collection="list" separator=";">
			UPDATE flow_task_actor 
			<set>
				<if test="taskActor.taskId != null">`task_id` = #{taskActor.taskId},</if>
				<if test="taskActor.varable != null">`varable` = #{taskActor.varable},</if>
				<if test="taskActor.url != null">`url` = #{taskActor.url},</if>
				<if test="taskActor.createDate != null">`create_date` = #{taskActor.createDate},</if>
				<if test="taskActor.createBy != null">`create_by` = #{taskActor.createBy},</if>
				<if test="taskActor.delFlag != null">`del_flag` = #{taskActor.delFlag}</if>
			</set>
			WHERE id = #{taskActor.id} 
		</foreach>
	</update>
	
